{
  "easycom": {
    "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
  },

  "pages": [{
    "path": "pages/index/index",
    "style": {
      "navigationBarTitleText": "首页",
      "app-plus": {
        "scrollIndicator": "none"
      }
    }
  }, {
    "path": "pages/select/select",
    "style": {
      "navigationBarTitleText": "精选",
      "enablePullDownRefresh": false
    }

  }, {
    "path": "pages/welfare/welfare",
    "style": {
      "navigationBarTitleText": "福利社",
      "enablePullDownRefresh": false
    }

  }, {
    "path": "pages/my/my",
    "style": {
      "navigationBarTitleText": "我的",
      "enablePullDownRefresh": false
    }

  }],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "mp-weixin": {
      "scrollIndicator": "none"
    }
  },
  "condition": {
    "current": 0,
    "list": [{
      "name": "",
      "path": "",
      "query": ""
    }]
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [{
      "pagePath": "pages/index/index",
      "iconPath": "./static/img/14.png",
      "selectedIconPath": "./static/img/6.png",
      "text": "首页"
    }, {
      "pagePath": "pages/select/select",
      "iconPath": "./static/img/7.png",
      "selectedIconPath": "./static/img/11.png",
      "text": "精选"
    }, {
      "pagePath": "pages/welfare/welfare",
      "iconPath": "./static/img/8.png",
      "selectedIconPath": "./static/img/12.png",
      "text": "福利社"
    }, {
      "pagePath": "pages/my/my",
      "iconPath": "./static/img/9.png",
      "selectedIconPath": "./static/img/13.png",
      "text": "我的"
    }]
  },
  "scrollIndicator": "none"
}